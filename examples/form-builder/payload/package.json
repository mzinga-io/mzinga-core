{
  "name": "mzinga-example-form-builder-cms",
  "description": "The CMS that utilizes MZinga's form builder plugin.",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_SEED=true PAYLOAD_DROP_DATABASE=true PAYLOAD_CONFIG_PATH=src/mzinga.config.ts nodemon",
    "build:mzinga": "cross-env PAYLOAD_CONFIG_PATH=src/mzinga.config.ts mzinga build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn build:mzinga && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/mzinga.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/mzinga.config.ts mzinga generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/mzinga.config.ts mzinga generate:graphQLSchema"
  },
  "dependencies": {
    "@mzinga/bundler-webpack": "latest",
    "@mzinga/db-mongodb": "latest",
    "@mzinga/richtext-slate": "latest",
    "@faceless-ui/modal": "2.0.2",
    "@mzinga/plugin-form-builder": "latest",
    "@mzinga/plugin-seo": "latest",
    "dotenv": "^8.2.0",
    "express": "^4.19.2",
    "mzinga": "latest"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
