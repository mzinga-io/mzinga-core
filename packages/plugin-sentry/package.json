{
  "name": "@mzinga/plugin-sentry",
  "version": "0.1.3",
  "homepage:": "https://mzinga.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/mzinga-io/mzinga-core.git",
    "directory": "packages/plugin-sentry"
  },
  "description": "Sentry plugin for MZinga",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm build:swc && pnpm build:types",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf {dist,*.tsbuildinfo}",
    "prepublishOnly": "pnpm clean && pnpm build",
    "version": "npm version"
  },
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "typescript",
    "sentry",
    "error handling"
  ],
  "author": "dev@payloadcms.com",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "mzinga": "^0.1.0",
    "react": "18.3.1"
  },
  "dependencies": {
    "@sentry/node": "^7.55.2",
    "@sentry/types": "^7.54.0",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@mzinga/eslint-config": "workspace:*",
    "@types/express": "^4.17.9",
    "@types/jest": "^29.5.2",
    "@types/node": "18.11.3",
    "@types/react": "18.3.1",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "17.2.3",
    "jest": "30.2.0",
    "mzinga": "workspace:*",
    "nodemon": "3.1.10",
    "ts-jest": "^29.4.0",
    "webpack": "5.102.1"
  }
}
