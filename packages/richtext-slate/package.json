{
  "name": "@mzinga/richtext-slate",
  "version": "0.1.3",
  "description": "The officially supported Slate richtext adapter for MZinga",
  "repository": {
    "type": "git",
    "url": "https://github.com/mzinga-io/mzinga-core.git",
    "directory": "packages/richtext-slate"
  },
  "license": "MIT",
  "homepage": "https://mzinga.io",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:swc && pnpm build:types",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf {dist,*.tsbuildinfo}",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "prepublishOnly": "pnpm clean && pnpm build",
    "version": "npm version"
  },
  "dependencies": {
    "@faceless-ui/modal": "2.0.2",
    "i18next": "22.5.1",
    "is-hotkey": "0.2.0",
    "react-i18next": "11.18.6",
    "slate": "0.91.4",
    "slate-history": "0.86.0",
    "slate-hyperscript": "0.81.3",
    "slate-react": "0.92.0"
  },
  "devDependencies": {
    "@mzinga/eslint-config": "workspace:*",
    "@types/node": "20.5.7",
    "@types/react": "18.3.1",
    "mzinga": "workspace:*"
  },
  "peerDependencies": {
    "mzinga": "^0.1.0",
    "react": "18.3.1"
  },
  "exports": {
    ".": {
      "default": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": null,
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ]
}
