    "mzinga": "^0.1.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "stripe": "^10.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.7",
    "@types/uuid": "^9.0.0",
    "@mzinga/eslint-config": "workspace:*",
    "@types/express": "^4.17.9",
    "@types/react": "18.0.21",
