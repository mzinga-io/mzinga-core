{
  "name": "@mzinga/plugin-redirects",
  "version": "0.0.6",
  "homepage:": "https://mzinga.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/mzinga-io/mzinga-core.git",
    "directory": "packages/plugin-redirects"
  },
  "description": "Redirects plugin for MZinga",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm copyfiles && pnpm build:swc && pnpm build:types",
    "build:swc": "swc ./src -d ./dist --config-file .swcrc",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "clean": "rimraf {dist,*.tsbuildinfo}",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json}\" dist/",
    "prepublishOnly": "pnpm clean && pnpm build",
    "version": "npm version"
  },
  "keywords": [
    "payload",
    "cms",
    "plugin",
    "typescript",
    "react",
    "redirects",
    "nextjs"
  ],
  "author": "dev@payloadcms.com",
  "license": "MIT",
  "peerDependencies": {
    "mzinga": "0.0.6",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@mzinga/eslint-config": "workspace:*",
    "@types/express": "^4.17.9",
    "@types/react": "18.0.21",
    "mzinga": "workspace:*"
  },
  "exports": {
    ".": {
      "default": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": null,
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist",
    "types.js",
    "types.d.ts"
  ]
}