    "prepublishOnly": "pnpm clean && pnpm build && pnpm test",
    "test": "jest"
  },
  "peerDependencies": {
    "payload": "^1.8.2 || ^2.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.289.0",
    "@aws-sdk/client-s3": "^3.142.0",
