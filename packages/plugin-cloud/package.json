    "@aws-sdk/lib-storage": "^3.267.0",
    "amazon-cognito-identity-js": "^6.1.2",
    "nodemailer": "7.0.5"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.1",
    "@types/nodemailer": "7.0.1",
    "payload": "workspace:*",
    "ts-jest": "^29.1.0",
    "webpack": "5.94.0"
  },
  "files": [
    "dist"
